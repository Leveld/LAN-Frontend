    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.7",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.16.2",
    "file-loader": "^1.1.6",
    "image-webpack-loader": "^4.0.0",
    "mongoose": "^5.0.2",
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-with-props": "^0.1.1",
    "react-scripts": "^1.1.0",
