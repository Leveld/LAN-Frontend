{
  "name": "capstone",
  "version": "0.0.1",
  "scripts": {
    "start": "node src/server/server.js",
    "watch": "nodemon src/server/server.js",
    "build": "eslint src/*.js && eslint src/*.jsx && webpack",
    "build-production": "npm run build -- -p",
    "dev-win": "start npm run build -- --watch && npm run watch",
    "dev-unix": "npm run build -- --watch & npm run watch",
    "devw": "npm run dev-win",
    "devm": "npm run dev-unix",
    "devl": "npm run dev-unix",
    "react-web": "react-scripts start"
  },
